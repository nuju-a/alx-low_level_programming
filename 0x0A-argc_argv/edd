#include "main.h"

/**
  *main - Entry point
  *@argc: argument count
  *@argv: argument array
  *
  *Return: 0
  */
int main(int argc, char *argv[])
{
	int sum = 0;
	int i;

	if (argc > 1)
	{
		for (i = 1; i < argc; i++)
		{
			char *num = argv[i];
			int j;
			for (j = 0; num[j] != '\0'; j++)
			{
				if (!isdigit(num[j]))
					printf("Error\n");
				return (1);
			}
			sum +=atoi(argv[i]);
		}
	printf("%d\n", sum);
	}
	else
	{
		printf("0\n");
	}
	return (0);
}
